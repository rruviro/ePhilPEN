<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.application.philpenriskassessment.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_splashFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.application.philpenriskassessment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_formFragment"
            app:destination="@id/formFragment" />
    </fragment>
    <fragment
        android:id="@+id/formFragment"
        android:name="com.application.philpenriskassessment.FormFragment"
        android:label="fragment_form"
        tools:layout="@layout/fragment_form_i" >
        <action
            android:id="@+id/action_formFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_formFragment_to_formIIFragment"
            app:destination="@id/formIIFragment" />
    </fragment>
    <fragment
        android:id="@+id/formIIFragment"
        android:name="com.application.philpenriskassessment.FormIIFragment"
        android:label="fragment_formii"
        tools:layout="@layout/fragment_form_ii" >
        <action
            android:id="@+id/action_formIIFragment_to_formFragment"
            app:destination="@id/formFragment" />
        <action
            android:id="@+id/action_formIIFragment_to_formIIIFragment"
            app:destination="@id/formIIIFragment" />
        <action
            android:id="@+id/action_formIIFragment_to_referFragment"
            app:destination="@id/referFragment" />
        <action
            android:id="@+id/action_formIIFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/formIIIFragment"
        android:name="com.application.philpenriskassessment.FormIIIFragment"
        android:label="fragment_form_i_i_i"
        tools:layout="@layout/fragment_form_iii" >
        <action
            android:id="@+id/action_formIIIFragment_to_formIIFragment"
            app:destination="@id/formIIFragment" />
        <action
            android:id="@+id/action_formIIIFragment_to_formIVFragment"
            app:destination="@id/formIVFragment" />
    </fragment>
    <fragment
        android:id="@+id/formIVFragment"
        android:name="com.application.philpenriskassessment.FormIVFragment"
        android:label="fragment_form_i_v"
        tools:layout="@layout/fragment_form_iv" >
        <action
            android:id="@+id/action_formIVFragment_to_formIIIFragment"
            app:destination="@id/formIIIFragment" />
        <action
            android:id="@+id/action_formIVFragment_to_formVFragment"
            app:destination="@id/formVFragment" />
    </fragment>
    <fragment
        android:id="@+id/formVFragment"
        android:name="com.application.philpenriskassessment.FormVFragment"
        android:label="fragment_form_v"
        tools:layout="@layout/fragment_form_v" >
        <action
            android:id="@+id/action_formVFragment_to_formIVFragment"
            app:destination="@id/formIVFragment" />
        <action
            android:id="@+id/action_formVFragment_to_formVIFragment"
            app:destination="@id/formVIFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.application.philpenriskassessment.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment_to_referFragment"
            app:destination="@id/referFragment" />
    </fragment>
    <fragment
        android:id="@+id/referFragment"
        android:name="com.application.philpenriskassessment.ReferFragment"
        android:label="fragment_refer"
        tools:layout="@layout/fragment_refer" >
        <action
            android:id="@+id/action_referFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/formVIFragment"
        android:name="com.application.philpenriskassessment.FormVIFragment"
        android:label="fragment_form_vi"
        tools:layout="@layout/fragment_form_vi" >
        <action
            android:id="@+id/action_formVIFragment_to_formVFragment"
            app:destination="@id/formVFragment" />
        <action
            android:id="@+id/action_formVIFragment_to_formVIIFragment"
            app:destination="@id/formVIIFragment" />
    </fragment>
    <fragment
        android:id="@+id/formVIIFragment"
        android:name="com.application.philpenriskassessment.FormVIIFragment"
        android:label="fragment_form_vii"
        tools:layout="@layout/fragment_form_vii" >
        <action
            android:id="@+id/action_formVIIFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_formVIIFragment_to_formVIFragment"
            app:destination="@id/formVIFragment" />
    </fragment>
</navigation>